<div class="container">
  <section class="row">
    <h2> Siebel </h2>

    <div class="row row_element">
      <div class="col-md-2">
        Solution ID:
      </div>
      <div class="col-md-3">
        {{solutionIdentifier}}
      </div>
      <div class="col-md-2"></div>
      <div class="col-md-2">
        Billing Account:
      </div>
      <div class="col-md-3">
        {{billingAccount}}
      </div>
    </div>

    <div class="row row_element">
      <div class="col-md-2">
        Service Account:
      </div>
      <div class="col-md-3">
        {{serviceAccount}}
      </div>
      <div class="col-md-2"></div>
      <div class="col-md-2">
        Service Account Site:
      </div>
      <div class="col-md-3">
        {{serviceAccountSite}}
      </div>
    </div>
  </section>
</div>

{{#link-to 'client-link.siebel.edit' model.id}}
<button type="button" class="btn btn-success pull-right"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit</button>
{{/link-to}}
