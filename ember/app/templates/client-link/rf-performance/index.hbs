<div class="container">
  <section class="row">
    <h2> RF Performance </h2>

    <div class="table-responsive" cellspacing="0" width="100%">
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>UL RSSI</th>
            <th>DL RSSI</th>
            <th>UL CNR</th>
            <th>DL CNR</th>
            <th>TX Power</th>
            <th>Step Attenuator</th>
          </tr>
        </thead>
        <tbody>
          {{#each parameter in radio.rfPerformanceParameters}}
            <tr>
              <td> name of modulation </td>
              <td>{{parameter.uplinkRssi}}</td>
              <td>{{parameter.downlinkRssi}}</td>
              <td>{{parameter.uplinkCnr}}</td>
              <td>{{parameter.downlinkCnr}}</td>
              <td>{{parameter.txPower}}</td>
              <td>{{parameter.stepAttenuator}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </section>
</div>

{{#link-to 'client-link.rf-performance.edit'}}
<button type="button" class="btn btn-primary pull-right"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit</button>
{{/link-to}}
