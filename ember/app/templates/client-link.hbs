    <div role="tabpanel" class="tab-pane fade" id="notes">
      <div class="container">
        <!-- SECTION 7: NOTES -->
        <section class="row">
          <h2> Notes </h2>
          <div class="col-md-12">
            <textarea rows="10"></textarea>
          </div>
        </section>
      </div>
