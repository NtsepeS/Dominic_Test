                <option value="4QAM2/3">4QAM2/3</option>
                <option value="4QAM">4QAM</option>
                <option value="16QAM">16QAM</option>
                <option value="64QAM">64QAM</option>
              </select>
            </div>
            <div class="col-md-2">
