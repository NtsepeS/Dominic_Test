<div role="tabpanel">

  <!-- Nav tabs -->
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a href="#site_information" aria-controls="site_information" role="tab" data-toggle="tab">Site Information</a></li>
    <li role="presentation"><a href="#siebel" aria-controls="siebel" role="tab" data-toggle="tab">Siebel</a></li>
    <li role="presentation"><a href="#rf_performance" aria-controls="rf_performance" role="tab" data-toggle="tab">RF Performance</a></li>
    <li role="presentation"><a href="#modulation" aria-controls="modulation" role="tab" data-toggle="tab">Modulation</a></li>
    <li role="presentation"><a href="#physical_ports" aria-controls="physical_ports" role="tab" data-toggle="tab">Physical Ports</a></li>
    <li role="presentation"><a href="#service_fragments" aria-controls="service_fragments" role="tab" data-toggle="tab">Service Fragments</a></li>
    <li role="presentation"><a href="#notes" aria-controls="notes" role="tab" data-toggle="tab">Notes</a></li>
    <li role="presentation"><a href="#quality_assurance" aria-controls="quality_assurance" role="tab" data-toggle="tab">Quality Assurance</a></li>
    <li role="presentation"><a href="#pictures" aria-controls="pictures" role="tab" data-toggle="tab">Pictures</a></li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane fade in active" id="site_information">
      <div class="container">
    <!-- SECTION 1: SITE INFORMATION -->
        <section class="row">
          <h2>Site Information</h2>
          <div class="row row_element">
            <div class="col-md-2">
              Client
            </div>
            <div class="col-md-3">
              <select>
                <option selected="selected"></option>
                <option value="1Stream Managed Technology Solutions">1Stream Managed Technology Solutions</option>
                <option value="2Go Interactive">2Go Interactive</option>
                <option value="Actom">Actom</option>
                <option value="Actvest">Actvest</option>
                <option value="Adcock Ingram Healthcare">Adcock Ingram Healthcare</option>
                <option value="Adcorp">Adcorp</option>
                <option value="Adcorp Shared Services">Adcorp Shared Services</option>
                <option value="Adcorp Shared Services Centre">Adcorp Shared Services Centre</option>
                <option value="ADT">ADT</option>
                <option value="Advtech">Advtech</option>
                <option value="Advtech Resourcing">Advtech Resourcing</option>
                <option value="AECI Limited">AECI Limited</option>
                <option value="Aegis Media Central Services">Aegis Media Central Services</option>
                <option value="African Sales Company">African Sales Company</option>
                <option value="Aidan Francis Casserley - Castlerock">Aidan Francis Casserley - Castlerock</option>
                <option value="Air Products">Air Products</option>
                <option value="Akacia">Akacia</option>
              </select>
            </div>
            <div class="col-md-2"></div>

            <div class="col-md-2">
              Branch Name
            </div>
            <div class="col-md-3">
              {{input value=branch}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Status
            </div>
            <div class="col-md-3">
              <select>
                <option selected="selected"></option>
                <option value="Commissioning">Commissioning</option>
                <option value="Deactived">Deactived</option>
                <option value="To Be Decommissioned">To Be Decommissioned</option>
                <option value="Decommissioned">Decommissioned</option>
                <option value="AUDIT IN PROCESS">AUDIT IN PROCESS</option>
                <option value="AUDIT DONE">AUDIT DONE</option>
              </select>
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Circuit Number
            </div>
            <div class="col-md-3">
              {{input value=circuitNumber}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Network Operator
            </div>
            <div class="col-md-3">
              {{view "Ember.Select"
                      contentBinding="networkOperators"
                      optionValuePath="content"
                      optionLabelPath="content.name"
                      valueBinding="networkOperator.content"
                    }}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              MSAD Number
            </div>
            <div class="col-md-3">
              {{input value=msadNumber}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Link Type
            </div>
            <div class="col-md-3">
              {{view "Ember.Select"
                      contentBinding="linkTypes"
                      optionValuePath="content"
                      optionLabelPath="content.name"
                      valueBinding="linkType.content"
                    }}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Activation Date
            </div>
            <div class="col-md-3">
              <!-- make a calendar popup -->
              {{input value=activationDate}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Sector Name
            </div>
            <div class="col-md-3">
              {{view "Ember.Select"
                      contentBinding="baseStationSectors"
                      optionValuePath="content"
                      optionLabelPath="content.name"
                      valueBinding="baseStationSector.content"
                    }}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Mac Address
            </div>
            <div class="col-md-3">
              {{input value=macAddress}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Antenna Size
            </div>
            <div class="col-md-3">
              {{view "Ember.Select"
                      contentBinding="antennas"
                      optionValuePath="content"
                      optionLabelPath="content.size"
                      valueBinding="antenna.content"
                    }}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Distance
            </div>
            <div class="col-md-3">
              {{input value=distance}}
            </div>
          </div>
        </section>
      </div>
    </div>
    <div role="tabpanel" class="tab-pane fade" id="siebel">
      <div class="container">
        <!-- SECTION 2: SIEBEL -->
        <section class="row">
          <h2> Siebel </h2>

          <div class="row row_element">
            <div class="col-md-2">
              Solution ID
            </div>
            <div class="col-md-3">
              {{input value=solutionIdentifier}}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Billing Account
            </div>
            <div class="col-md-3">
              {{input value=billingAccount}}
            </div>
          </div>

          <div class="row row_element">
            <div class="col-md-2">
              Service Account
            </div>
            <div class="col-md-3">
              {{input value=serviceAccount}}
            </div>
            <div class="col-md-2"></div>
            <div class="col-md-2">
              Service Account Site
            </div>
            <div class="col-md-3">
              {{input value=serviceAccountSite}}
            </div>
          </div>
        </section>
      </div>
    </div>
    <div role="tabpanel" class="tab-pane fade" id="rf_performance">
      <div class="container">
        <!-- SECTION 3: RF PERFORMANCE -->
        <section class="row">
          <h2> RF Performance </h2>
          <div class="table-responsive">

            <table class="table table-responsive display" cellspacing="0" width="100%">
              <thead>
                <tr>
                  <th></th>
                  <th>RSSI</th>
                  <th>CNR</th>
