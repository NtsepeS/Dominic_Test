                  <th>CNR</th>
                  <th>Tx Power</th>
                  <th>Step Attenuator</th>
                  <th>Reserve Power</th>
              </thead>
              <tbody>
                <tr>
                  <td>Predicted</td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                </tr>
                <tr>
                  <td>Commissioned</td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                  <td><input type="number"></input></td>
                </tr>
                <tr class="success">
                  <td>Result</td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td></td>
                </tr>
              </tbody>
            </table>
          </div>

        </section>
      </div>
    </div>
